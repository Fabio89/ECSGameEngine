<Window x:Class="Editor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Editor"
        xmlns:ui="http://schemas.modernwpf.com/2019"
        mc:Ignorable="d"
        Height="1080" Width="1920"
        Title="Mashi Engine">
    <DockPanel LastChildFill="False">
        <local:MainMenu DockPanel.Dock="Top"></local:MainMenu>
        <local:Viewport x:Name="Viewport" DockPanel.Dock="Top" Height="720" HorizontalAlignment="Stretch" VerticalAlignment="Stretch"></local:Viewport>
        <local:AssetBrowser HorizontalAlignment="Stretch" Height="800" DockPanel.Dock="Bottom"></local:AssetBrowser>
    </DockPanel>
</Window>
